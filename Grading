# Create an array of scores for students
scores = [85, 92, 78, 90, 88, 76, 95, 89, 80, 87]

highscore = 0

# Calculate the average score
average_score = sum(scores) / len(scores)
print("Average Score:", average_score)

# Find the highest score
for score in scores:
    if score > highscore:
        highscore = score
print("Highest Score:", highscore)

# Find the lowest score
lowscore = highscore
for score in scores:
    if score < lowscore:
        lowscore = score
print("Lowest Score:", lowscore)

# Determine the letter grade for each score
for score in scores:
        if score > 90:
            print("You got a A")
        elif score > 80:
            print("You got a B")
        elif score > 70:
            print("You got a C")
        elif score > 60:
            print("You got a D")
        else:
            print("You got a F")
